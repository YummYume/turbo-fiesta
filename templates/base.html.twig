{% set themeMode = app.request.cookies.get('theme_mode') ?? 'light' %}
{% set isLight = 'light' in themeMode %}

<!DOCTYPE html>
<html
    class="{{ isLight ? 'light' : 'dark' }}"
    data-theme="{{ isLight ? 'light' : 'dark' }}"
    lang="{{ app.request.locale ?? app.request.defaultLocale }}"
    {{ stimulus_controller('theme') }}
    data-theme-dark-mode-class="dark"
    data-theme-light-mode-class="light"
    data-theme-hidden-class="opacity-0 !h-0 pointer-events-none"
    data-theme-uri-value="{{ url("app_change_theme") }}"
>
    <head>
        <meta charset="UTF-8" />
        <title>
            {% apply spaceless %}
                {% block title %}
                    {{ 'common.app_name'|trans }}
                {% endblock %}
            {% endapply %}
        </title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta name="description" content="{% apply spaceless %}{% block description %}{{ 'common.description'|trans }}{% endblock %}{% endapply %}">
        {% block icon %}
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}">
        {% endblock %}
        {% block meta %}{% endblock %}
        {% block stylesheets %}
            {{ encore_entry_link_tags("app") }}
        {% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags("app") }}
        {% endblock %}
    </head>
    <body class="flex flex-col bg-neutral-200 min-h-screen text-neutral-900 dark:bg-neutral-900 dark:text-neutral-100">
        {% block header %}
            <header data-controller="drawer">
                {% block flashes %}
                    <div id="app-flashes" class="fixed bottom-3 left-2 right-2 z-[1000] mr-auto flex max-w-lg flex-col gap-2 transition-all">
                        {% for type, messages in app.flashes %}
                            {% for message in messages %}
                                {{ component("alert", { message, type }) }}
                            {% endfor %}
                        {% endfor %}
                    </div>
                {% endblock %}

                {% block nav %}
                    <nav class="bg-neutral-100 border-neutral-200 px-4 lg:px-6 py-2.5 dark:bg-neutral-800">
                        <div class="flex flex-wrap justify-between items-center">
                            <div class="flex justify-start items-center">
                                {# Sidebar button #}
                                <button
                                    class="hidden p-2 mr-3 text-neutral-600 rounded cursor-pointer lg:inline hover:text-neutral-900 hover:bg-neutral-100 dark:text-neutral-400 dark:hover:text-neutral-100 dark:hover:bg-neutral-700"
                                    data-action="click->drawer#show"
                                    type="button"
                                >
                                <svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M3 5a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 10a1 1 0 011-1h6a1 1 0 110 2H4a1 1 0 01-1-1zM3 15a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"></path></svg>
                                </button>

                                {# Sidebar mobile button #}
                                <button
                                    aria-expanded="false"
                                    class="p-2 mr-2 text-neutral-600 rounded-lg cursor-pointer lg:hidden hover:text-neutral-900 hover:bg-neutral-100 focus:bg-neutral-100 dark:focus:bg-neutral-700 focus:ring-2 focus:ring-neutral-100 dark:focus:ring-neutral-700 dark:text-neutral-400 dark:hover:bg-neutral-700 dark:hover:text-neutral-100"
                                    data-action="click->drawer#show"
                                >
                                    <svg aria-hidden="true" class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M3 5a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 10a1 1 0 011-1h6a1 1 0 110 2H4a1 1 0 01-1-1zM3 15a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"></path></svg>
                                    <svg aria-hidden="true" class="hidden w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
                                    <span class="sr-only">Toggle sidebar</span>
                                </button>

                                {# Logo #}
                                <a href="{{ path('app_homepage') }}" class="flex mr-4">
                                    <img src="https://flowbite.s3.amazonaws.com/logo.svg" class="mr-3 h-8" alt="FlowBite Logo" />
                                    <span class="self-center text-2xl font-semibold whitespace-nowrap dark:text-neutral-100">Flowbite</span>
                                </a>

                                {# Search #}
                                <div data-controller="modal">
                                    <form action="{{ path("app_search") }}" method="get">
                                        <label for="topbar-search" class="sr-only">Search</label>
                                        <div class="relative lg:w-96">
                                            <div class="flex absolute inset-y-0 left-0 items-center pl-3 pointer-events-none">
                                                <svg class="w-5 h-5 text-neutral-500 dark:text-neutral-400" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M8 4a4 4 0 100 8 4 4 0 000-8zM2 8a6 6 0 1110.89 3.476l4.817 4.817a1 1 0 01-1.414 1.414l-4.816-4.816A6 6 0 012 8z" clip-rule="evenodd"></path></svg>
                                            </div>
                                            <input
                                                aria-label="{{ "common.search"|trans }}"
                                                class="bg-neutral-50 border border-neutral-300 text-neutral-900 sm:text-sm rounded-lg focus:ring-secondary-500 focus:border-secondary-500 block w-full pl-10 p-2.5 dark:bg-neutral-700 dark:border-neutral-600 dark:placeholder-neutral-400 dark:text-neutral-100 dark:focus:ring-secondary-500 dark:focus:border-secondary-500"
                                                data-action="click->modal#show"
                                                name="q"
                                                placeholder="{{ "common.search"|trans }}..."
                                                type="search"
                                            >
                                        </div>
                                    </form>
                                    <form
                                        aria-hidden="true"
                                        action="{{ path("app_search") }}"
                                        class="fixed top-0 left-0 right-0 z-50 hidden w-full p-4 overflow-x-hidden overflow-y-auto md:inset-0 h-modal md:h-full"
                                        data-action="click->modal#hide:self"
                                        data-modal-target="modal"
                                        method="get"
                                        role="dialog"
                                        tabindex="-1"
                                    >
                                        <div class="relative w-full h-full max-w-md md:h-auto">
                                            <div class="relative bg-neutral-100 rounded-lg shadow dark:bg-neutral-800 p-6">
                                                <div class="flex items-start justify-between rounded-t mb-2">
                                                    <h3 class="text-lg font-normal text-neutral-500 dark:text-neutral-400">
                                                        {{ "common.global_search"|trans }}
                                                    </h3>
                                                    <button
                                                        class="text-neutral-400 bg-transparent hover:bg-neutral-200 hover:text-neutral-900 rounded-lg text-sm p-1.5 ml-auto inline-flex items-center dark:hover:bg-neutral-600 dark:hover:text-neutral-100"
                                                        data-action="click->modal#hide"
                                                        type="button"
                                                    >
                                                        {{ component('icon', { icon: 'x-mark', class: 'h-5 w-5' }) }}

                                                        <span class="sr-only">Close modal</span>
                                                    </button>
                                                </div>
                                                <div>
                                                    {{ component('global_search') }}
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="flex items-center lg:order-2" data-controller="tooltip">
                                {# Mobile search #}
                                <button type="button" class="p-2 text-neutral-500 rounded-lg lg:hidden hover:text-neutral-900 hover:bg-neutral-100 dark:text-neutral-400 dark:hover:bg-neutral-700 dark:hover:text-neutral-100">
                                    <span class="sr-only">Search</span>
                                    <svg aria-hidden="true" class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M8 4a4 4 0 100 8 4 4 0 000-8zM2 8a6 6 0 1110.89 3.476l4.817 4.817a1 1 0 01-1.414 1.414l-4.816-4.816A6 6 0 012 8z" clip-rule="evenodd"></path></svg>
                                </button>

                                {# Theme switcher #}
                                <a
                                    class="p-2 mr-1 text-neutral-500 rounded-lg hover:text-neutral-900 hover:bg-neutral-100 dark:text-neutral-400 dark:hover:text-neutral-100 dark:hover:bg-neutral-700 focus:ring-4 focus:ring-neutral-300 dark:focus:ring-neutral-600"
                                    data-tooltip-target="trigger"
                                    role="button"
                                    href="{{ path('app_change_theme') }}"
                                >
                                    <svg
                                        data-theme-target="light"
                                        class="{{ isLight ? '' : 'opacity-0 pointer-events-none !h-0' }} w-6 h-6 transition-all"
                                        fill="currentColor"
                                        xmlns="http://www.w3.org/2000/svg"
                                        viewBox="0 0 24 24"
                                    >
                                        <path d="M5.64,17l-.71.71a1,1,0,0,0,0,1.41,1,1,0,0,0,1.41,0l.71-.71A1,1,0,0,0,5.64,17ZM5,12a1,1,0,0,0-1-1H3a1,1,0,0,0,0,2H4A1,1,0,0,0,5,12Zm7-7a1,1,0,0,0,1-1V3a1,1,0,0,0-2,0V4A1,1,0,0,0,12,5ZM5.64,7.05a1,1,0,0,0,.7.29,1,1,0,0,0,.71-.29,1,1,0,0,0,0-1.41l-.71-.71A1,1,0,0,0,4.93,6.34Zm12,.29a1,1,0,0,0,.7-.29l.71-.71a1,1,0,1,0-1.41-1.41L17,5.64a1,1,0,0,0,0,1.41A1,1,0,0,0,17.66,7.34ZM21,11H20a1,1,0,0,0,0,2h1a1,1,0,0,0,0-2Zm-9,8a1,1,0,0,0-1,1v1a1,1,0,0,0,2,0V20A1,1,0,0,0,12,19ZM18.36,17A1,1,0,0,0,17,18.36l.71.71a1,1,0,0,0,1.41,0,1,1,0,0,0,0-1.41ZM12,6.5A5.5,5.5,0,1,0,17.5,12,5.51,5.51,0,0,0,12,6.5Zm0,9A3.5,3.5,0,1,1,15.5,12,3.5,3.5,0,0,1,12,15.5Z"/>
                                    </svg>
                                    <svg
                                        data-theme-target="dark"
                                        class="{{ not isLight ? '' : 'opacity-0 pointer-events-none !h-0' }} w-6 h-6 transition-all"
                                        fill="currentColor"
                                        xmlns="http://www.w3.org/2000/svg"
                                        viewBox="0 0 24 24"
                                    >
                                        <path d="M21.64,13a1,1,0,0,0-1.05-.14,8.05,8.05,0,0,1-3.37.73A8.15,8.15,0,0,1,9.08,5.49a8.59,8.59,0,0,1,.25-2A1,1,0,0,0,8,2.36,10.14,10.14,0,1,0,22,14.05,1,1,0,0,0,21.64,13Zm-9.5,6.69A8.14,8.14,0,0,1,7.08,5.22v.27A10.15,10.15,0,0,0,17.22,15.63a9.79,9.79,0,0,0,2.1-.22A8.11,8.11,0,0,1,12.14,19.73Z"/>
                                    </svg>
                                </a>
                                {{ component('tooltip', { text: 'common.switch_theme'|trans }) }}

                                {# Notifications #}
                                <div data-controller="dropdown">
                                    <button
                                        class="p-2 mr-1 text-neutral-500 rounded-lg hover:text-neutral-900 hover:bg-neutral-100 dark:text-neutral-400 dark:hover:text-neutral-100 dark:hover:bg-neutral-700 focus:ring-4 focus:ring-neutral-300 dark:focus:ring-neutral-600"
                                        data-dropdown-target="trigger"
                                        type="button"
                                    >
                                        <span class="sr-only">View notifications</span>
                                        <svg aria-hidden="true" class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M10 2a6 6 0 00-6 6v3.586l-.707.707A1 1 0 004 14h12a1 1 0 00.707-1.707L16 11.586V8a6 6 0 00-6-6zM10 18a3 3 0 01-3-3h6a3 3 0 01-3 3z"></path></svg>
                                    </button>

                                    <div
                                        class="hidden overflow-hidden z-50 my-4 max-w-sm text-base list-none bg-neutral-100 rounded divide-y divide-neutral-100 shadow-lg dark:divide-neutral-600 dark:bg-neutral-700"
                                        data-dropdown-target="dropdown"
                                    >
                                        <div class="block py-2 px-4 text-base font-medium text-center text-neutral-700 bg-neutral-50 dark:bg-neutral-700 dark:text-neutral-400">
                                            Notifications
                                        </div>
                                        <div>
                                        <a href="#" class="flex py-3 px-4 border-b hover:bg-neutral-100 dark:hover:bg-neutral-600 dark:border-neutral-600">
                                            <div class="flex-shrink-0">
                                            <img class="w-11 h-11 rounded-full" src="https://flowbite.s3.amazonaws.com/blocks/marketing-ui/avatars/bonnie-success.png" alt="Bonnie Green avatar">
                                            <div class="flex absolute justify-center items-center ml-6 -mt-5 w-5 h-5 rounded-full border border-neutral-100 bg-secondary-700 dark:border-neutral-700">
                                                <svg aria-hidden="true" class="w-3 h-3 text-neutral-100" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M8.707 7.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l2-2a1 1 0 00-1.414-1.414L11 7.586V3a1 1 0 10-2 0v4.586l-.293-.293z"></path><path d="M3 5a2 2 0 012-2h1a1 1 0 010 2H5v7h2l1 2h4l1-2h2V5h-1a1 1 0 110-2h1a2 2 0 012 2v10a2 2 0 01-2 2H5a2 2 0 01-2-2V5z"></path></svg>
                                            </div>
                                            </div>
                                            <div class="pl-3 w-full">
                                                <div class="text-neutral-500 font-normal text-sm mb-1.5 dark:text-neutral-400">New message from <span class="font-semibold text-neutral-900 dark:text-neutral-100">Bonnie Green</span>: "Hey, what's up? All set for the presentation?"</div>
                                                <div class="text-xs font-medium text-secondary-700 dark:text-secondary-400">a few moments ago</div>
                                            </div>
                                        </a>
                                        <a href="#" class="flex py-3 px-4 border-b hover:bg-neutral-100 dark:hover:bg-neutral-600 dark:border-neutral-600">
                                            <div class="flex-shrink-0">
                                            <img class="w-11 h-11 rounded-full" src="https://flowbite.s3.amazonaws.com/blocks/marketing-ui/avatars/jese-leos.png" alt="Jese Leos avatar">
                                            <div class="flex absolute justify-center items-center ml-6 -mt-5 w-5 h-5 bg-neutral-900 rounded-full border border-neutral-100 dark:border-neutral-700">
                                                <svg aria-hidden="true" class="w-3 h-3 text-neutral-100" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M8 9a3 3 0 100-6 3 3 0 000 6zM8 11a6 6 0 016 6H2a6 6 0 016-6zM16 7a1 1 0 10-2 0v1h-1a1 1 0 100 2h1v1a1 1 0 102 0v-1h1a1 1 0 100-2h-1V7z"></path></svg>
                                            </div>
                                            </div>
                                            <div class="pl-3 w-full">
                                                <div class="text-neutral-500 font-normal text-sm mb-1.5 dark:text-neutral-400"><span class="font-semibold text-neutral-900 dark:text-neutral-100">Jese leos</span> and <span class="font-medium text-neutral-900 dark:text-neutral-100">5 others</span> started following you.</div>
                                                <div class="text-xs font-medium text-secondary-700 dark:text-secondary-400">10 minutes ago</div>
                                            </div>
                                        </a>
                                        <a href="#" class="flex py-3 px-4 border-b hover:bg-neutral-100 dark:hover:bg-neutral-600 dark:border-neutral-600">
                                            <div class="flex-shrink-0">
                                            <img class="w-11 h-11 rounded-full" src="https://flowbite.s3.amazonaws.com/blocks/marketing-ui/avatars/joseph-mcfall.png" alt="Joseph McFall avatar">
                                            <div class="flex absolute justify-center items-center ml-6 -mt-5 w-5 h-5 bg-red-600 rounded-full border border-neutral-100 dark:border-neutral-700">
                                                <svg aria-hidden="true" class="w-3 h-3 text-neutral-100" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M3.172 5.172a4 4 0 015.656 0L10 6.343l1.172-1.171a4 4 0 115.656 5.656L10 17.657l-6.828-6.829a4 4 0 010-5.656z" clip-rule="evenodd"></path></svg>
                                            </div>
                                            </div>
                                            <div class="pl-3 w-full">
                                                <div class="text-neutral-500 font-normal text-sm mb-1.5 dark:text-neutral-400"><span class="font-semibold text-neutral-900 dark:text-neutral-100">Joseph Mcfall</span> and <span class="font-medium text-neutral-900 dark:text-neutral-100">141 others</span> love your story. See it and view more stories.</div>
                                                <div class="text-xs font-medium text-secondary-700 dark:text-secondary-400">44 minutes ago</div>
                                            </div>
                                        </a>
                                        <a href="#" class="flex py-3 px-4 border-b hover:bg-neutral-100 dark:hover:bg-neutral-600 dark:border-neutral-600">
                                            <div class="flex-shrink-0">
                                            <img class="w-11 h-11 rounded-full" src="https://flowbite.s3.amazonaws.com/blocks/marketing-ui/avatars/roberta-casas.png" alt="Roberta Casas image">
                                            <div class="flex absolute justify-center items-center ml-6 -mt-5 w-5 h-5 bg-success rounded-full border border-neutral-100 dark:border-neutral-700">
                                                <svg aria-hidden="true" class="w-3 h-3 text-neutral-100" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M18 13V5a2 2 0 00-2-2H4a2 2 0 00-2 2v8a2 2 0 002 2h3l3 3 3-3h3a2 2 0 002-2zM5 7a1 1 0 011-1h8a1 1 0 110 2H6a1 1 0 01-1-1zm1 3a1 1 0 100 2h3a1 1 0 100-2H6z" clip-rule="evenodd"></path></svg>
                                            </div>
                                            </div>
                                            <div class="pl-3 w-full">
                                                <div class="text-neutral-500 font-normal text-sm mb-1.5 dark:text-neutral-400"><span class="font-semibold text-neutral-900 dark:text-neutral-100">Leslie Livingston</span> mentioned you in a comment: <span class="font-medium text-secondary-700 dark:text-secondary-500">@bonnie.green</span> what do you say?</div>
                                                <div class="text-xs font-medium text-secondary-700 dark:text-secondary-400">1 hour ago</div>
                                            </div>
                                        </a>
                                        <a href="#" class="flex py-3 px-4 hover:bg-neutral-100 dark:hover:bg-neutral-600">
                                            <div class="flex-shrink-0">
                                            <img class="w-11 h-11 rounded-full" src="https://flowbite.s3.amazonaws.com/blocks/marketing-ui/avatars/robert-brown.png" alt="Robert image">
                                            <div class="flex absolute justify-center items-center ml-6 -mt-5 w-5 h-5 bg-purple-500 rounded-full border border-neutral-100 dark:border-neutral-700">
                                                <svg aria-hidden="true" class="w-3 h-3 text-neutral-100" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M2 6a2 2 0 012-2h6a2 2 0 012 2v8a2 2 0 01-2 2H4a2 2 0 01-2-2V6zM14.553 7.106A1 1 0 0014 8v4a1 1 0 00.553.894l2 1A1 1 0 0018 13V7a1 1 0 00-1.447-.894l-2 1z"></path></svg>
                                            </div>
                                            </div>
                                            <div class="pl-3 w-full">
                                                <div class="text-neutral-500 font-normal text-sm mb-1.5 dark:text-neutral-400"><span class="font-semibold text-neutral-900 dark:text-neutral-100">Robert Brown</span> posted a new video: Glassmorphism - learn how to implement the new design trend.</div>
                                                <div class="text-xs font-medium text-secondary-700 dark:text-secondary-400">3 hours ago</div>
                                            </div>
                                        </a>
                                        </div>
                                        <a href="#" class="block py-2 text-base font-normal text-center text-neutral-900 bg-neutral-50 hover:bg-neutral-100 dark:bg-neutral-700 dark:text-neutral-100 dark:hover:underline">
                                            <div class="inline-flex items-center ">
                                            <svg aria-hidden="true" class="mr-2 w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M10 12a2 2 0 100-4 2 2 0 000 4z"></path><path fill-rule="evenodd" d="M.458 10C1.732 5.943 5.522 3 10 3s8.268 2.943 9.542 7c-1.274 4.057-5.064 7-9.542 7S1.732 14.057.458 10zM14 10a4 4 0 11-8 0 4 4 0 018 0z" clip-rule="evenodd"></path></svg>
                                            View all
                                            </div>
                                        </a>
                                    </div>
                                </div>

                                {# User #}
                                {% include "common/_user_dropdown.html.twig" with { permanent: true } only %}
                            </div>
                        </div>
                    </nav>
                {% endblock %}

                {# Drawer #}
                <aside
                    class="fixed top-0 left-0 z-40 w-full h-screen max-w-xs p-4 overflow-y-auto transition-transform -translate-x-full bg-neutral-100 dark:bg-neutral-800"
                    data-drawer-target="drawer"
                    tabindex="-1"
                >
                    <button
                        class="text-neutral-400 bg-transparent hover:bg-neutral-200 hover:text-neutral-900 rounded-lg text-sm p-1.5 absolute top-2.5 right-2.5 inline-flex items-center dark:hover:bg-neutral-600 dark:hover:text-neutral-100"
                        data-action="click->drawer#hide"
                        type="button"
                    >
                        <svg
                            aria-hidden="true"
                            class="w-5 h-5"
                            fill="currentColor"
                            viewBox="0 0 20 20"
                            xmlns="http://www.w3.org/2000/svg"
                        >
                            <path
                                clip-rule="evenodd"
                                d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z"
                                fill-rule="evenodd"
                            >
                            </path>
                        </svg>
                        <span class="sr-only">Close menu</span>
                    </button>
                    <h5 class="inline-flex items-center mb-4 text-base font-semibold text-neutral-500 uppercase dark:text-neutral-400">
                        Menu
                    </h5>
                    <ul class="space-y-2">
